                               <div class="float-left q-pt-xs q-mt-xs">&nbsp;&nbsp;-&nbsp;</div>
                                   <div class="float-left">
                                        <q-input rounded class="q-pl-sm"   outlined style="margin:0;padding:0;width:120px" label="Area Code" dense>
                                        </q-input>
                                   </div>

                                    <div class="float-left q-pt-xs q-mt-xs">&nbsp;&nbsp;-&nbsp;</div>
                                   <div class="float-left">
                                        <q-input rounded class="q-pl-sm"   outlined style="margin:0;padding:0;width:190px" label="Phone Number" dense>
                                        </q-input>
                                   </div>
                                   <div class="float-left ">
                                        <div class="float-left q-pt-xs q-mt-xs">&nbsp;&nbsp;x&nbsp;</div>
