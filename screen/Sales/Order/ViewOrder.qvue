                            <div class="col-3 q-pl-sm">
                                <q-input rounded  outlined style="margin:0;padding:0;" label="" dense>
                                </q-input>
                            </div>
                            <div class="col-2 q-pl-sm">
                                <q-input rounded  outlined style="margin:0;padding:0;" label="" dense>
                                </q-input>
