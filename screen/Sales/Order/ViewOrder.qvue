                                    dense
                                    outlined
                                    v-model="new_state"
                                    use-input
                                    input-debounce="700"
                                    :class="!$q.dark.isActive?'bg-white':''"
                                    input-debounce="0"
                                    option-value="value"
                                    option-label="label"
                                    :options="state_options"
                                    @filter="filterFnState"
                                  >
                                    <template v-slot:no-option>
